<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="@dimen/top_bordebtn"
        android:left="@dimen/left_bordebtn">
        <shape>
            <solid android:color="@color/grisClaro" />
            <corners android:radius="@dimen/radius_bordebtn" />
        </shape>

    </item>
    <item
        android:bottom="@dimen/botton_bordebtn"
        android:right="@dimen/right_bordebtn">
        <shape>
            <solid android:color="@color/morado" />
            <corners android:radius="@dimen/corner_boton_empezar" />
            <stroke
                android:width="@dimen/width_bordebtn"
                android:color="@color/white" />
        </shape>
    </item>
</layer-list>