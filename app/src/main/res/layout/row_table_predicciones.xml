<?xml version="1.0" encoding="utf-8"?>

<TableRow xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/row_encabezado"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"

    >

    <TextView
        android:id="@+id/column_object_id"
        style="@style/EstiloTextViewEncabezadoTabla"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:gravity="center_horizontal"
        android:text="@string/object_id"
         />

    <TextView
        android:id="@+id/column_fecha"
        style="@style/EstiloTextViewEncabezadoTabla"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:text="@string/fechal_lanzamiento_text"/>

    <TextView
        android:id="@+id/column_prediccion"
        style="@style/EstiloTextViewEncabezadoTabla"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@color/lavanda"
        android:fontFamily="@font/oswald_semibold"
        android:text="@string/prediccion_text"/>
</TableRow>

