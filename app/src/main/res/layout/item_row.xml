<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android">

<TableRow
        android:id="@+id/row_content_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/textViewobject_id"
            style="@style/EstiloTextViewContenidoTabla"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@string/id_text"
            android:textAlignment="viewStart" />

        <TextView
            android:id="@+id/textViewapogee"
            style="@style/EstiloTextViewContenidoTabla"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@string/valor_text"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/textViewperigee"
            style="@style/EstiloTextViewContenidoTabla"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@string/valor_text" />


        <TextView
            android:id="@+id/textViewperiod"
            style="@style/EstiloTextViewContenidoTabla"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:text="@string/valor_text" />

    </TableRow>
</layout>
